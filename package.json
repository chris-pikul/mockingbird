{
    "name": "mockingbird",
    "displayName": "Mockingbird",
    "description": "Generate data for mocks and testing with ease",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": {
            "properties": {
                "mockingbird.contextMenu.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enables the use of the context-menu to quickly insert data"
                }
            }
        },
        "commands": [
            {
                "command": "mb.date-time.currentISOTimestamp",
                "title": "Insert: Current ISO Timestamp",
                "shortTitle": "ISO Timestamp"
            },
            {
                "command": "mb.date-time.currentISODate",
                "title": "Insert: Current ISO Date",
                "shortTitle": "ISO Date"
            },
            {
                "command": "mb.date-time.currentISOTime",
                "title": "Insert: Current ISO Time",
                "shortTitle": "ISO Time"
            },
            {
                "command": "mb.date-time.currentLocaleTimestamp",
                "title": "Insert: Current Timestamp Using Local Locale",
                "shortTitle": "Timestamp Local Locale"
            },
            {
                "command": "mb.date-time.currentLocaleDate",
                "title": "Insert: Current Date Using Local Locale",
                "shortTitle": "Date Local Locale"
            },
            {
                "command": "mb.date-time.currentLocaleTime",
                "title": "Insert: Current Time Using Local Locale",
                "shortTitle": "Date Local Locale"
            },
            {
                "command": "mb.date-time.currentFormatted",
                "title": "Insert: Current Date/Time With Formatting",
                "shortTitle": "Formatted Date/Time"
            },
            {
                "command": "mb.number.pi",
                "title": "Insert: Pi",
                "shortTitle": "Pi"
            },
            {
                "command": "mb.number.e",
                "title": "Insert: Euler's Number",
                "shortTitle": "Euler"
            },
            {
                "command": "mb.number.epsilon",
                "title": "Insert: Epsilon (Really Small Number)",
                "shortTitle": "Epsilon"
            },
            {
                "command": "mb.number.randomUnit",
                "title": "Random: Unit Float (0..1)",
                "shortTitle": "Random Unit"
            },
            {
                "command": "mb.number.randomPositiveFloat",
                "title": "Random: Positive Float (0..MAX)",
                "shortTitle": "Random Positive Float"
            },
            {
                "command": "mb.number.randomNegativeFloat",
                "title": "Random: Negative Float (0..-MAX)",
                "shortTitle": "Random Negative Float"
            },
            {
                "command": "mb.number.randomFloat",
                "title": "Random: Complex Float (-MAX/2..MAX/2)",
                "shortTitle": "Random Complex Float"
            },
            {
                "command": "mb.number.randomSimpleFloat",
                "title": "Random: Simple Float",
                "shortTitle": "Random Float"
            },
            {
                "command": "mb.number.randomFloatRange",
                "title": "Random: Float in Range",
                "shortTitle": "Random Float"
            },
            {
                "command": "mb.number.randomPositiveInteger",
                "title": "Random: Positive Integer",
                "shortTitle": "Random Positive Integer"
            },
            {
                "command": "mb.number.randomNegativeInteger",
                "title": "Random: Negative Integer",
                "shortTitle": "Random Negative Integer"
            },
            {
                "command": "mb.number.randomInteger",
                "title": "Random: Integer",
                "shortTitle": "Random Integer"
            },
            {
                "command": "mb.number.randomInteger32",
                "title": "Random: 32bit Integer",
                "shortTitle": "Random Integer 32"
            },
            {
                "command": "mb.number.randomIntegerRange",
                "title": "Random: Integer Range",
                "shortTitle": "Random Integer Range"
            },
            {
                "command": "mb.number.randomByte",
                "title": "Random: Byte (Unsigned)",
                "shortTitle": "Random Byte"
            },
            {
                "command": "mb.number.randomSignedByte",
                "title": "Random: Byte (Signed)",
                "shortTitle": "Random Signed Byte"
            },
            {
                "command": "mb.number.randomShort",
                "title": "Random: Short (16bit Integer)",
                "shortTitle": "Random Short"
            },
            {
                "command": "mb.number.randomSignedShort",
                "title": "Random: Signed Short (16bit Integer)",
                "shortTitle": "Random Signed Short"
            },
            {
                "command": "mb.number.randomHexInteger16",
                "title": "Random: 16bit Hexidecimal Integer",
                "shortTitle": "Random 16bit Hex"
            },
            {
                "command": "mb.number.randomHexInteger24",
                "title": "Random: 24bit Hexidecimal Integer",
                "shortTitle": "Random 24bit Hex"
            },
            {
                "command": "mb.number.randomHexInteger32",
                "title": "Random: 32bit Hexidecimal Integer",
                "shortTitle": "Random 32bit Hex"
            },
            {
                "command": "mb.number.randomDigit",
                "title": "Random: Digit (0-9)",
                "shortTitle": "Random Digit"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "submenu": "mockingbird",
                    "group": "navigation",
                    "when": "config.mockingbird.contextMenu.enabled"
                }
            ],
            "mockingbird": [
                {
                    "submenu": "mockingbird/date-time"
                },
                {
                    "submenu": "mockingbird/number"
                }
            ],
            "mockingbird/date-time": [
                {
                    "command": "mb.date-time.currentISOTimestamp"
                },
                {
                    "command": "mb.date-time.currentISODate"
                },
                {
                    "command": "mb.date-time.currentISOTime"
                },
                {
                    "command": "mb.date-time.currentLocaleTimestamp"
                },
                {
                    "command": "mb.date-time.currentLocaleDate"
                },
                {
                    "command": "mb.date-time.currentLocaleTime"
                },
                {
                    "command": "mb.date-time.currentFormatted"
                }
            ],
            "mockingbird/number": [
                {
                    "command": "mb.number.pi"
                },
                {
                    "command": "mb.number.e"
                },
                {
                    "command": "mb.number.epsilon"
                },
                {
                    "command": "mb.number.randomUnit"
                },
                {
                    "command": "mb.number.randomPositiveFloat"
                },
                {
                    "command": "mb.number.randomNegativeFloat"
                },
                {
                    "command": "mb.number.randomFloat"
                },
                {
                    "command": "mb.number.randomSimpleFloat"
                },
                {
                    "command": "mb.number.randomFloatRange"
                },
                {
                    "command": "mb.number.randomPositiveInteger"
                },
                {
                    "command": "mb.number.randomNegativeInteger"
                },
                {
                    "command": "mb.number.randomInteger"
                },
                {
                    "command": "mb.number.randomInteger32"
                },
                {
                    "command": "mb.number.randomIntegerRange"
                },
                {
                    "command": "mb.number.randomByte"
                },
                {
                    "command": "mb.number.randomSignedByte"
                },
                {
                    "command": "mb.number.randomShort"
                },
                {
                    "command": "mb.number.randomSignedShort"
                },
                {
                    "command": "mb.number.randomHexInteger16"
                },
                {
                    "command": "mb.number.randomHexInteger24"
                },
                {
                    "command": "mb.number.randomHexInteger32"
                },
                {
                    "command": "mb.number.randomDigit"
                }
            ]
        },
        "submenus": [
            {
                "id": "mockingbird",
                "label": "Mockingbird"
            },
            {
                "id": "mockingbird/date-time",
                "label": "Date Time"
            },
            {
                "id": "mockingbird/number",
                "label": "Number"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "pnpm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "commands": "node ./scripts/fill-commands.mjs"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^4.2.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.80.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "prettier": "^3.0.0",
        "ts-loader": "^9.4.3",
        "typescript": "^5.1.3",
        "webpack": "^5.85.0",
        "webpack-cli": "^5.1.1"
    },
    "dependencies": {
        "date-fns": "^2.30.0"
    }
}