{
    "name": "mockingbird",
    "displayName": "Mockingbird",
    "description": "Generate data for mocks and testing with ease",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "configuration": {
            "properties": {
                "mockingbird.contextMenu.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enables the use of the context-menu to quickly insert data"
                },
                "mockingbird.date-time.randomTimezone": {
                    "type": "boolean",
                    "default": true,
                    "description": "Randomize the timezone in generated timestamps, if false it will use the local systems timezone"
                },
                "mockingbird.numbers.hexPrefix": {
                    "scope": "language-overridable",
                    "type": "string",
                    "title": "Hexidecimal Number Prefix",
                    "description": "Prefix given to hexidecimal number generation",
                    "examples": [
                        "0x",
                        "#"
                    ],
                    "default": "0x"
                },
                "mockingbird.numbers.octalPrefix": {
                    "scope": "language-overridable",
                    "type": "string",
                    "title": "Octal Number Prefix",
                    "description": "Prefix given to octal number generation",
                    "examples": [
                        "0o",
                        "0"
                    ],
                    "default": "0o"
                },
                "mockingbird.numbers.binaryPrefix": {
                    "scope": "language-overridable",
                    "type": "string",
                    "title": "Binary Number Prefix",
                    "description": "Prefix given to binary number generation",
                    "examples": [
                        "0b"
                    ],
                    "default": "0b"
                }
            }
        },
        "commands": [
            {
                "command": "mb.date-time.currentISOTimestamp",
                "title": "Current ISO Timestamp",
                "shortTitle": "ISO Timestamp",
                "category": "Insert"
            },
            {
                "command": "mb.date-time.currentISODate",
                "title": "Current ISO Date",
                "shortTitle": "ISO Date",
                "category": "Insert"
            },
            {
                "command": "mb.date-time.currentISOTime",
                "title": "Current ISO Time",
                "shortTitle": "ISO Time",
                "category": "Insert"
            },
            {
                "command": "mb.date-time.currentLocaleTimestamp",
                "title": "Current Timestamp Using Local Locale",
                "shortTitle": "Timestamp Local Locale",
                "category": "Insert"
            },
            {
                "command": "mb.date-time.currentLocaleDate",
                "title": "Current Date Using Local Locale",
                "shortTitle": "Date Local Locale",
                "category": "Insert"
            },
            {
                "command": "mb.date-time.currentLocaleTime",
                "title": "Current Time Using Local Locale",
                "shortTitle": "Date Local Locale",
                "category": "Insert"
            },
            {
                "command": "mb.date-time.currentFormatted",
                "title": "Current Date/Time With Formatting",
                "shortTitle": "Formatted Date/Time",
                "category": "Insert"
            },
            {
                "command": "mb.date-time.randomYear",
                "title": "Year (1900-Now)",
                "shortTitle": "Random Year",
                "category": "Random"
            },
            {
                "command": "mb.date-time.randomYearCustom",
                "title": "Year With Custom Range",
                "shortTitle": "Random Year Custom",
                "category": "Random"
            },
            {
