{
    "name": "mockingbird",
    "displayName": "Mockingbird",
    "description": "Generate data for mocks and testing with ease",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.80.0"
    },
    "publisher": "chris-pikul",
    "author": {
        "name": "Chris Pikul",
        "email": "ChrisPikul510@gmail.com"
    },
    "categories": [
        "Other",
        "Testing",
        "Snippets"
    ],
    "main": "./dist/extension.js",
    "license": "Apache-2.0",
    "keywords": [
        "random",
        "generation",
        "mock",
        "data",
        "placeholder"
    ],
    "galleryBanner": {
        "theme": "dark",
        "color": "#596673"
    },
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "icon": "static/logo512.png",
    "homepage": "https://github.com/chris-pikul/mockingbird/blob/main/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/chris-pikul/mockingbird.git"
    },
    "bugs": {
        "url": "https://github.com/chris-pikul/mockingbird/issues"
    },
    "contributes": {
        "configuration": {
            "title": "Mockingbird",
            "properties": {
                "mockingbird.contextMenu.enabled": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enables the use of the context-menu to quickly insert data"
                },
                "mockingbird.date-time.randomTimezone": {
                    "type": "boolean",
                    "default": true,
                    "description": "Randomize the timezone in generated timestamps, if false it will use the local systems timezone"
                },
                "mockingbird.numbers.hexPrefix": {
                    "scope": "language-overridable",
                    "type": "string",
                    "title": "Hexidecimal Number Prefix",
                    "description": "Prefix given to hexidecimal number generation",
                    "examples": [
                        "0x",
                        "#"
                    ],
                    "default": "0x"
                },
                "mockingbird.numbers.octalPrefix": {
                    "scope": "language-overridable",
                    "type": "string",
                    "title": "Octal Number Prefix",
                    "description": "Prefix given to octal number generation",
                    "examples": [
                        "0o",
                        "0"
                    ],
                    "default": "0o"
                },
                "mockingbird.numbers.binaryPrefix": {
                    "scope": "language-overridable",
                    "type": "string",
                    "title": "Binary Number Prefix",
                    "description": "Prefix given to binary number generation",
                    "examples": [
                        "0b"
                    ],
                    "default": "0b"
                },
                "mockingbird.text.lorem_ipsum.sentenceWordsMin": {
                    "type": "integer",
                    "title": "Minimum Words In Sentences",
                    "description": "Minimum number of words that can be generated when building a Lorem Ipsum sentence.",
                    "default": 3,
                    "minimum": 1
                },
                "mockingbird.text.lorem_ipsum.sentenceWordsMax": {
                    "type": "integer",
                    "title": "Maximum Words In Sentences",
                    "description": "Maximum number of words that can be generated when building a Lorem Ipsum sentence.",
                    "default": 10,
                    "minimum": 2
                },
                "mockingbird.text.lorem_ipsum.paragraphSentencesMin": {
                    "type": "integer",
                    "title": "Minimum Sentences In a Paragraph",
                    "description": "Minimum number of sentences that can be generated when building a Lorem Ipsum paragraph.",
                    "default": 2,
                    "minimum": 1
                },
                "mockingbird.text.lorem_ipsum.paragraphSentencesMax": {
                    "type": "integer",
                    "title": "Maximum Sentences In a Paragraph",
                    "description": "Maximum number of sentences that can be generated when building a Lorem Ipsum paragraph.",
                    "default": 6,
                    "minimum": 2
                },
                "mockingbird.presets.regularExpression": {
                    "type": "object",
                    "title": "Regular Expression Presets",
                    "markdownDescription": "Used as a database of available Regular Expression presets that you have created. It is prefered that you use the provided favorites editor to change this value. Otherwise, the schema is presented in the [documentation](https://github.com/chris-pikul/mockingbird).",
                    "default": {}
                }
            }
        },
